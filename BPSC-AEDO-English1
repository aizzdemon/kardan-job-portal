            q46: "c", q47: "c", q48: "a", q49: "b", q50: "a",
            q51: "c", q52: "b", q53: "d", q54: "b", q55: "d",
            q56: "a", q57: "c", q58: "b", q59: "c", q60: "d",
            q61: "a", q62: "b", q63: "b", q64: "c", q65: "a",
