            q71: "c", q72: "d", q73: "a", q74: "d", q75: "c",
            q76: "b", q77: "b", q78: "d", q79: "c", q80: "a",
            q81: "a", q82: "b", q83: "b", q84: "a", q85: "b",
            q86: "b", q87: "b", q88: "d", q89: "c", q90: "b",
            q91: "b", q92: "a", q93: "a", q94: "b", q95: "b",
            q96: "a", q97: "b", q98: "d", q99: "b", q100: "b"
        };
        
        function checkAnswers() {
            const form = document.getElementById('exam-form');
            const questions = Object.keys(correctAnswers);
            let score = 0;

            questions.forEach(qName => {
                const selectedOption = form.elements[qName].value;
                const correctAnswer = correctAnswers[qName];
                const questionDiv = document.querySelector(`input[name="${qName}"]`).closest('.question-card');
                
                // Reset classes
                const allLabels = questionDiv.querySelectorAll('.option-label');
                allLabels.forEach(label => label.classList.remove('correct-answer', 'wrong-answer'));

                // Find the correct option label
                const correctLabel = document.querySelector(`input[name="${qName}"][value="${correctAnswer}"]`).closest('.option-label');
                correctLabel.classList.add('correct-answer');

                // Check the user's answer
                if (selectedOption) {
                    const selectedLabel = document.querySelector(`input[name="${qName}"][value="${selectedOption}"]`).closest('.option-label');
                    if (selectedOption === correctAnswer) {
                        score++;
                    } else {
                        selectedLabel.classList.add('wrong-answer');
                    }
                }
            });

            document.getElementById('score-display').textContent = `Your Score: ${score} / ${questions.length}`;
            window.scrollTo({ top: document.body.scrollHeight, behavior: 'smooth' });
        }
    </script>

</body>
</html>
